---
import TechnologiesCarousel from "./TechnologyItem.astro";

const technologies = [
    {
        label: "Angular",
        svg: `<svg width="60" height="60" viewBox="0 0 60 60" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
<path d="M0 0H60V60H0V0Z" fill="url(#pattern0_154_13)"/>
<defs>
<pattern id="pattern0_154_13" patternContentUnits="objectBoundingBox" width="1" height="1">
<use xlink:href="#image0_154_13" transform="scale(0.01)"/>
</pattern>
<image id="image0_154_13" width="100" height="100" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
</defs>
</svg>`,
    },
    {
        label: "React",
        svg: `<svg width="60" height="60" viewBox="0 0 60 60" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
<rect width="60" height="60" fill="url(#pattern0_96_9)"/>
<defs>
<pattern id="pattern0_96_9" patternContentUnits="objectBoundingBox" width="1" height="1">
