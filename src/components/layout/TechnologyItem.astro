---
export interface Props {
  svg: string;
  label: string;
}

const { svg, label } = Astro.props;
---


<div class="flex flex-col items-center cursor-pointer transition-transform duration-200 hover:-translate-y-0.5">
  <div class="w-12 h-12 mb-2 flex items-center justify-center text-white" set:html={svg}>
  </div>
</div>